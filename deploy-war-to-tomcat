node{
   stage('SCM Checkout'){
     git 'https://github.com/Manikavala/my-app'
   }
   stage('Compile-Package'){
      // Get maven home path
       def mvnHome = tool name: 'Maven_internal', type: 'maven'   
      sh "${mvnHome}/bin/mvn package"
   }
   stage('Deploy to Tomcat'){
      
      sshagent(['ssh-key-1']) {
         sh 'scp -o StrictHostKeyChecking=no target/*.war tomcat@192.168.43.246:Tomcat8/apache-tomcat-8.5.31/webapps'
      }
   }
  
}
